{
  "functions": {
    "api/index.ts": {
      "runtime": "@vercel/node@4.2.4"  // Gunakan runtime resmi dengan versi
    }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/index.ts" },
    { "src": "/(.*)", "dest": "/client/index.html" }
  ],
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node@4.2.4"
    }
  ]
}
